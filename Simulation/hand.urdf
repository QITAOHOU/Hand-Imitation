<robot name="Hand">

    <link name="base">
        <origin xyz="0 0 0" />
    </link>
    <link name="arm">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" /> 
            <geometry>
                <mesh filename="meshes/arm_reduced.stl" scale=".0005 .0005 .0015"/>
            </geometry>
            <material name="Material"/>
        </visual>
    </link>

    <joint name="armbase" type="fixed">
        <parent link="base" />
        <child link="arm" />
    </joint>

    <link name="palm" >
        <visual> 
            <geometry>
                <mesh filename="meshes/palm_reduced.stl" scale=".005 .005 .005"/>
            </geometry>
            <material name="Material"/>
        </visual>
    </link>
    
    <joint name="wrist" type="fixed">
        <origin xyz="-0.05 0 2.5" rpy="0 0 0" />
        <parent link="arm" />
        <child link="palm" />
    </joint>


    <!-- Thumb-->
    <link name="thumb0">
        <visual>
            <geometry>
                <mesh filename="meshes/finger_long_reduced.stl" scale=".005 .005 .005"/>
            </geometry>
            <material name="Material"/>
        </visual>
    </link>
    <link name="thumb1">
        <visual>
            <geometry>
                <mesh filename="meshes/finger_long_reduced.stl" scale=".005 .005 .005"/>
            </geometry>
            <material name="Material"/>
        </visual>
    </link>

    <joint name="thumbpalm" type="revolute">
        <parent link="palm" />
        <child link="thumb0" />
        <origin xyz="-0.2 0 -0.45" rpy="0 -0.80 0" />
        <axis xyz="0.5 0 0.5" />
        <limit effort="1000.0" velocity="10.0"/>
    </joint>
    <joint name="thumb_0_1" type="revolute">
        <parent link="thumb0" />
        <child link="thumb1" />
        <origin xyz="0 0 0.3" rpy="0 0 0"/>
        <axis xyz="1 0 0" />
        <limit effort="1000.0" velocity="10.0"/>
    </joint>


    <!-- index -->
    <link name="index0">
        <visual>
            <geometry>
                <mesh filename="meshes/finger_long_reduced.stl" scale=".005 .005 .005"/>
            </geometry>
            <material name="Material"/>
        </visual>
    </link>
    <link name="index1">
        <visual>
            <geometry>
                <mesh filename="meshes/finger_long_reduced.stl" scale=".005 .005 .005"/>
            </geometry>
            <material name="Material"/>
        </visual>
    </link>

    <joint name="indexpalm" type="revolute">
        <parent link="palm" />
        <child link="index0" />
        <origin xyz="-0.15 0 0.35" rpy="0 -0.1 0"/>
        <axis xyz="0.98 0 0.1" />
        <limit effort="1000.0" velocity="10.0"/>
    </joint>
    <joint name="index_0_1" type="revolute">
        <parent link="index0" />
        <child link="index1" />
        <origin xyz="0 0 0.3" rpy="0 0 0"/>
        <axis xyz="1 0 0" />
        <limit effort="1000.0" velocity="10.0"/>
    </joint>


    <!-- middle -->
    <link name="middle0">
        <visual>
            <geometry>
                <mesh filename="meshes/finger_long_reduced.stl" scale=".005 .005 .005"/>
            </geometry>
            <material name="Material"/>
        </visual>
    </link>
    <link name="middle1">
        <visual>
            <geometry>
                <mesh filename="meshes/finger_long_reduced.stl" scale=".005 .005 .005"/>
            </geometry>
            <material name="Material"/>
        </visual>
    </link>

    <joint name="middlepalm" type="revolute">
        <parent link="palm" />
        <child link="middle0" />
        <origin xyz="0.075 0 0.4" rpy="0 0 0"/>
        <axis xyz="1 0 0" />
        <limit effort="1000.0" velocity="10.0"/>
    </joint>
    <joint name="middle_0_1" type="revolute">
        <parent link="middle0" />
        <child link="middle1" />
        <origin xyz="0 0 0.3" rpy="0 0 0"/>
        <axis xyz="1 0 0" />
        <limit effort="1000.0" velocity="10.0"/>
    </joint>


    <!-- ring -->
    <link name="ring0">
        <visual>
            <geometry>
                <mesh filename="meshes/finger_long_reduced.stl" scale=".005 .005 .005"/>
            </geometry>
            <material name="Material"/>
        </visual>
    </link>
    <link name="ring1">
        <visual>
            <geometry>
                <mesh filename="meshes/finger_long_reduced.stl" scale=".005 .005 .005"/>
            </geometry>
            <material name="Material"/>
        </visual>
    </link>

    <joint name="ringpalm" type="revolute">
        <parent link="palm" />
        <child link="ring0" />
        <origin xyz="0.3 0 0.35" rpy="0 0.2 0"/>
        <axis xyz="0.98 0 -0.1" />
        <limit effort="1000.0" velocity="10.0"/>
    </joint>
    <joint name="ring_0_1" type="revolute">
        <parent link="ring0" />
        <child link="ring1" />
        <origin xyz="0 0 0.3" rpy="0 0 0"/>
        <axis xyz="1 0 0" />
        <limit effort="1000.0" velocity="10.0"/>
    </joint>

    <!-- little -->
    <link name="little0">
        <visual>
            <geometry>
                <mesh filename="meshes/finger_long_reduced.stl" scale=".005 .005 .005"/>
            </geometry>
            <material name="Material"/>
        </visual>
    </link>
    <link name="little1">
        <visual>
            <geometry>
                <mesh filename="meshes/finger_short_reduced.stl" scale=".005 .005 .005"/>
            </geometry>
            <material name="Material"/>
        </visual>
    </link>

    <joint name="littlepalm" type="revolute">
        <parent link="palm" />
        <child link="little0" />
        <origin xyz="0.5 0 0.2" rpy="0 0.5 0"/>
        <axis xyz="0.9 0 -0.27" />
        <limit effort="1000.0" velocity="10.0"/>
    </joint>
    <joint name="little_0_1" type="revolute">
        <parent link="little0" />
        <child link="little1" />
        <origin xyz="0 0 0.3" rpy="0 0 0"/>
        <axis xyz="1 0 0" />
        <limit effort="1000.0" velocity="10.0"/>
    </joint>

</robot>